{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "ignorePatterns": ["**/interface"],
  "rules": {
    "no-unused-vars": "off",
    "typescript/no-unused-vars": ["error"],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "semi": ["error", "never"]
  }
}
